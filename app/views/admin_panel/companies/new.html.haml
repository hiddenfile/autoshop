=render :partial => 'head', :locals => {:title => 'Создание записи'}

= form_for @company, :url => admin_panel_companies_path, :html => {:multipart => true} do |f|
  =render :partial => 'shared/errors', :locals => {:object => @company }
  =render :partial => 'form', :locals => {:f => f}
  = f.submit 'Добавить'